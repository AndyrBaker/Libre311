{
  "name": "localmotionwemove",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --port=3000",
    "build": "vite build",
    "preview": "vite preview",
    "start": "concurrently \"npm run start:frontend\" \"cross-env CONCURRENTLY_NEW_WINDOW=1 npm run start:backend\"",
    "start:frontend": "VITE_BACKEND_URL=http://localhost:8080/api npm run dev -- --open --host",
    "start:backend": "cd .. && MICRONAUT_ENVIRONMENTS=dev-data ./gradlew app:run"
  },
  "devDependencies": {
    "@playwright/test": "^1.32.3",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.5.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "svelte": "^3.54.0",
    "svelte-inview": "^4.0.0",
    "vite": "^4.2.0"
  },
  "type": "module",
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.15.1",
    "axios": "^1.3.5",
    "flatpickr": "^4.6.13",
    "fontfaceobserver": "^2.3.0",
    "svelte-multiselect": "^8.6.2"
  }
}
