		"@sveltejs/kit": "^1.5.0",
		"concurrently": "^8.0.1",
		"cross-env": "^7.0.3",
		"svelte": "^3.54.0",
		"svelte-inview": "^4.0.0",
		"vite": "^4.2.0"
	},
	"type": "module",
	"dependencies": {
		"@googlemaps/js-api-loader": "^1.15.1",
		"axios": "^1.3.5",
		"flatpickr": "^4.6.13"
	}
}
